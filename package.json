{
  "name": "ngx-stickybits",
  "version": "0.1.0",
  "description": "Angular sticky directive using Stickybits, a lightweight alternative to `position: sticky` polyfills",
  "homepage": "https://github.com/jfcere/ngx-stickybits",
  "license": "MIT",
  "author": {
    "name": "Jean-Francois Cere",
    "email": "jfcere@sherweb.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfcere/ngx-stickybits"
  },
  "keywords": [
    "angular",
    "ngx",
    "stickybits",
    "sticky"
  ],
  "scripts": {
    "ng": "ng",
    "start": "yarn link:lib && ng serve",
    "build:demo": "ng build demo",
    "build:lib": "ng build lib",
    "postbuild:lib": "cpx ./README.md ./dist/lib",
    "link:lib": "rimraf node_modules/ngx-stickybits && linklocal",
    "lint": "yarn lint:lib && yarn lint:demo",
    "lint:demo": "ng lint demo",
    "lint:lib": "ng lint lib",
    "lint:ci": "ng lint lib --format checkstyle > tslint.xml",
    "test": "ng test",
    "coveralls": "cat \"./coverage/lcov.info\" | \"./node_modules/coveralls/bin/coveralls.js\"",
    "gh-pages:build": "yarn build:demo --prod --aot --base-href /ngx-stickybits/",
    "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"CircleCI\" --email=\"circleci@email.com\" --no-silent",
    "publish:lib": "npm publish ./dist/lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "ngx-stickybits": "file:./lib",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "~4.5.0",
    "coveralls": "^2.13.3",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "linklocal": "^2.8.2",
    "ng-packagr": "^4.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^5.2.0",
    "typescript": "~3.2.2"
  }
}
